[include]
  path = ~/.user.gitconfig

[alias]
  co = checkout
  s = status
  ci = commit
  d = diff
  ds = diff --staged
  br = branch
  lc = log -n 1
  poh = push -u origin HEAD

[core]
  editor = vim
  ignoreCase = true
  pager      = less -F -X
  safecrlf   = false
  trustctime = false
  commentchar = %

[push]
	default = matching
